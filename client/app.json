{
  "expo": {
    "name": "cross-post",
    "slug": "cross-post",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "scheme": "crosspost",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#022c22"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.crosspost",
      "usesAppleSignIn": true
    },
    "android": {
      "package": "com.yourcompany.crosspost",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#022c22"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-apple-authentication",
      "expo-web-browser"
    ]
  }
}
